{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/bin/Debug/netcoreapp1.1/fangraph-priceguide-generator.dll",
            "args": ["16","/Users/eric.neunaber/Downloads/master.csv","/Users/eric.neunaber/Downloads/IdConversion-FULL-New.csv","/Users/eric.neunaber/Downloads/baseballdatabank-2017.1/core/Appearances.csv","/Users/eric.neunaber/Downloads/FanGraphs Leaderboard.csv"],
            "cwd": "${workspaceRoot}",
            "externalConsole": false,
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command.pickProcess}"
        }
    ]
}