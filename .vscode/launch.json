{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/bin/Debug/netcoreapp2.0/fangraph-priceguide-generator.dll",
            "args": ["convert-fangraph",
                "-py=2016",
                "-m=/Users/eric.neunaber/Downloads/master.csv",
                "-l=/Users/eric.neunaber/Downloads/baseballdatabank-2017.1/core/Appearances.csv",
                "-s=/Users/eric.neunaber/Downloads/",
                "-f=/Users/eric.neunaber/Downloads/2017BattingZips.csv",
                "-f=/Users/eric.neunaber/Downloads/2017PitchingZips.csv"],
            "cwd": "${workspaceRoot}",
            "externalConsole": false,
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command.pickProcess}"
        }
    ]
}